package naveenlabJava;

public class operator {

	public static void main(String[] args) {
		
		int a =40;
		int b = 20;
		if(a>b) 
		{
			System.out.println("a is greater " + a);
		}
		else
		{
			System.out.println("b is greater "+b);
		}

		
	int c = 50;
	int d = 40;
	if(c == d)
	{
		System.out.println("c and d are equal");
	}
	else
	{
		System.out.println("c and d are  not equal");
	}
	
	
	int e = 20;
	int f = 20;
	if (e != f)
	{
		System.out.println("e is not equal to f");
	}
	else
	{
		System.out.println("e is equal to f ");
	}
	
	int g = 20;
	int h = 20;
	if (g >= h)
	{
		System.out.println("g is greater then or equal to h");
	}
	else 
	{
		System.out.println("g is not greater then or equal to f ");
	}
	
	}

}
