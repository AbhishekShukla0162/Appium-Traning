 class Stidents{
	 int id;
	 String name;
	 static String CollegeName = "AKTU";
	 
	 Stidents(int i , String n)
	 {
		  id =  i;
		 name = n;
	 }
	 void Display(){
	 System.out.println(id +"" + name);
	 }
}
public class StaticKeyWord {

	public static void main(String[] args) {
		
		Stidents s = new Stidents(1,"abhishek");
		Stidents ss = new Stidents(2,"Amit");
		s.Display();
		ss.Display();
	}

}
