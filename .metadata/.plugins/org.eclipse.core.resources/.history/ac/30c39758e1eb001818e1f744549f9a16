package methodoiveridding;

class Car
{
	void start() 
	{
		System.out.println("car Start ");
	}
}

class Bnw extends Car
{
	void start() 
	{
		System.out.println("BmD starts ");
	}
}


public class TestInheritance {

	public static void main(String[] args)
	{
	 // Bnw b = new Bnw();
		// b.start();

	  Car t = new Car();
	  t.start();
	 
	}

}
