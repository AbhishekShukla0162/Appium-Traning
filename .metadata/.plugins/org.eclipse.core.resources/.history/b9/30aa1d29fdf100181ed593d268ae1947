package array;

public class Single {

	public static void main(String[] args) {
	
		
		int ary  [] = {2,3,4,5,6,7,8,9,10};
		System.out.println(ary[0]+ " " +ary [5]);
		
		for (int temp : ary) 
		{
			System.out.println(temp);
		}
		
		int x[] = new int[5];
		x[0] = 10;
		x[4] = 40;
		
		for ( int temp3 : x) 
		{
			System.out.println(temp3);
		}
		
		
	}

}
