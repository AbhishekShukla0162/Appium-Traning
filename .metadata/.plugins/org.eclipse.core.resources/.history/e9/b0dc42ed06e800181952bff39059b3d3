package Encapsulation;

public class TestEncapsulation  {

	public static void main(String[] args) {
	

		BaseEncapsulation b = new BaseEncapsulation();
		b.setRollNo(1);
		b.setName("ABhishek");
		b.setEMAIL("Abhsihek.SHUKLA");
		b.setMobile(856500944);
		b.setSalery(false);
		
		System.out.println(b.getRollNo());
		System.out.println(b.getName());
		System.out.println(b.getEMAIL());
		System.out.println(b.getMobile());
		System.out.println(b.getSalery());
		
		
	}

}
