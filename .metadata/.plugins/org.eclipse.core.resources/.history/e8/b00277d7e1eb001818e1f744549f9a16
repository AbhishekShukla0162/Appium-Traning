package methodoiveridding;

class Car
{
	void start() 
	{
		System.out.println("car Start ");
	}
	
	void stop() 
	{
		System.out.println("car Stop ");
	}
}

class Bnw extends Car
{
	void start() 
	{
		System.out.println("BmD starts ");
	}
	void stop() 
	{
		System.out.println("bmd  Stop ");
	}
}


public class TestInheritance {

	public static void main(String[] args)
	{
	  Bnw b = new Bnw();
		b.start();
		b.stop();

	 
	 
	}

}
